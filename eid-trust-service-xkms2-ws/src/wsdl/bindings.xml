<?xml version="1.0"?>
<jxb:bindings version="2.1" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

	<jxb:bindings schemaLocation="http://www.w3.org/TR/xkms2/Schemas/xkms.xsd"
		node="/xs:schema">
		<jxb:schemaBindings>
			<jxb:package name="be.fedict.trust.client.jaxb.xkms" />
		</jxb:schemaBindings>
		<jxb:bindings
			schemaLocation="http://www.w3.org/TR/2002/REC-xmldsig-core-20020212/xmldsig-core-schema.xsd"
			node="/xs:schema">
			<jxb:schemaBindings>
				<jxb:package name="be.fedict.trust.client.jaxb.xmldsig" />
			</jxb:schemaBindings>
		</jxb:bindings>
		<jxb:bindings schemaLocation="http://www.w3.org/TR/xmlenc-core/xenc-schema.xsd"
			node="/xs:schema">
			<jxb:schemaBindings>
				<jxb:package name="be.fedict.trust.client.jaxb.xenc" />
			</jxb:schemaBindings>
		</jxb:bindings>
		<jxb:bindings schemaLocation="http://uri.etsi.org/01903/v1.3.2/XAdES.xsd"
			node="/xs:schema">
			<jxb:schemaBindings>
				<jxb:package name="be.fedict.trust.client.jaxb.xades132" />
			</jxb:schemaBindings>
		</jxb:bindings>
	</jxb:bindings>

</jxb:bindings>