	<name>FedICT eID Trust Service EAR</name>
	<description>Java EE 5 EAR for eID Trust Service</description>
	<groupId>be.fedict.eid-trust-service</groupId>
	<artifactId>eid-trust-service-deploy</artifactId>
	<packaging>ear</packaging>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-ear-plugin</artifactId>
				<configuration>
					<modules>
						<sarModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-service</artifactId>
						</sarModule>
						<jarModule>
							<groupId>be.fedict.eid-applet</groupId>
							<artifactId>eid-applet-service</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>be.fedict.eid-applet</groupId>
							<artifactId>eid-applet-shared</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>be.fedict.eid-applet</groupId>
							<artifactId>eid-applet-service-spi</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>be.fedict</groupId>
							<artifactId>jtrust</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<ejbModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-entity</artifactId>
						</ejbModule>
						<ejbModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-model</artifactId>
						</ejbModule>
						<webModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-startup-runtime
							</artifactId>
						</webModule>
						<webModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-ws</artifactId>
							<contextRoot>/eid-trust-service-ws</contextRoot>
						</webModule>
						<webModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-ocsp-webapp</artifactId>
							<contextRoot>/eid-trust-service-ocsp</contextRoot>
						</webModule>
						<ejbModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-portal-model
							</artifactId>
						</ejbModule>
						<webModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-portal-webapp
							</artifactId>
							<contextRoot>/eid-trust-service-portal</contextRoot>
						</webModule>
						<ejbModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-admin-portal-model
							</artifactId>
						</ejbModule>
						<webModule>
							<groupId>be.fedict.eid-trust-service</groupId>
							<artifactId>eid-trust-service-admin-portal-webapp
							</artifactId>
							<contextRoot>/eid-trust-service-admin-portal
							</contextRoot>
						</webModule>
						<jarModule>
							<groupId>org.jboss.el</groupId>
							<artifactId>jboss-el</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>org.richfaces.framework</groupId>
							<artifactId>richfaces-api</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>org.richfaces.framework</groupId>
							<artifactId>richfaces-impl-jsf2</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>org.richfaces.ui</groupId>
							<artifactId>richfaces-ui</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>com.sun.facelets</groupId>
							<artifactId>jsf-facelets</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>org.snmp4j</groupId>
							<artifactId>snmp4j</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>
						<jarModule>
							<groupId>org.snmp4j</groupId>
							<artifactId>snmp4j-agent</artifactId>
							<includeInApplicationXml>true
							</includeInApplicationXml>
							<bundleDir>lib</bundleDir>
						</jarModule>

					</modules>
					<version>5</version>
					<earSourceDirectory>${project.build.outputDirectory}
					</earSourceDirectory>
					<defaultLibBundleDir>lib</defaultLibBundleDir>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>jboss-maven-plugin</artifactId>
				<configuration>
					<fileNames>
						<fileName>
							${basedir}/target/${project.artifactId}-${project.version}.ear
						</fileName>
					</fileNames>
				</configuration>
			</plugin>
		</plugins>
	</build>
