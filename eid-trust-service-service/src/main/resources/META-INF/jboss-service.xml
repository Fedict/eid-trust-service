<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server
    PUBLIC "-//JBoss//DTD MBean Service 5.0//EN"
    "http://www.jboss.org/j2ee/dtd/jboss-service_5_0.dtd">
<server>
	<mbean code="org.jboss.mq.server.jmx.Queue" name="trust:service=Queue,name=harvester">
		<attribute name="JNDIName">queue/trust/harvester</attribute>
		<depends optional-attribute-name="DestinationManager">
			jboss.mq:service=DestinationManager</depends>
	</mbean>

	<mbean code="org.jboss.security.auth.login.DynamicLoginConfig"
		name="trust:service=DynamicLoginConfig">
		<attribute name="AuthConfig">
			META-INF/trust-service-admin-login-config.xml
		</attribute>
		<depends optional-attribute-name="LoginConfigService">
			jboss.security:service=XMLLoginConfig
		</depends>
		<depends optional-attribute-name="SecurityManagerService">
			jboss.security:service=JaasSecurityManager
		</depends>
	</mbean>

</server>