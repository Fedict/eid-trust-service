<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<!-- Facelets configuration -->
	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>

	<!-- Configuration page navigation rules -->
	<navigation-rule>
		<from-view-id>/configuration.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/configuration.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/localization.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/localization.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/configuration.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/configuration.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<!-- END Administrator pages navigation rules -->

	<!-- Administrator pages navigation rules -->
	<navigation-rule>
		<from-view-id>/admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/admin.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<!-- END Administrator pages navigation rules -->

	<!--  Trust domain / points navigation rules -->
	<navigation-rule>
		<from-view-id>/trust-domain.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>modify</from-outcome>
			<to-view-id>/trust-domain-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/trust-domain.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/trust-domain-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>save</from-outcome>
			<to-view-id>/trust-domain.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>view</from-outcome>
			<to-view-id>/trust-point-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/trust-domain.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>select</from-outcome>
			<to-view-id>/trust-point-select.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/trust-domain-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/trust-point-edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/trust-point.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>back_trustdomain</from-outcome>
			<to-view-id>/trust-domain-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/trust-point.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>refresh</from-outcome>
			<to-view-id>/trust-point-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>success_trustdomain</from-outcome>
			<to-view-id>/trust-domain-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/trust-point.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>modify</from-outcome>
			<to-view-id>/trust-point-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/trust-point.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/trust-point-select.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/trust-domain-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/trust-domain-edit.seam</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<!--  END Trust domain / points navigation rules -->
</faces-config>