	</description>
	<groupId>be.fedict.eid-trust-service</groupId>
	<artifactId>eid-trust-service-ws</artifactId>
	<packaging>war</packaging>
	<dependencies>
		<dependency>
			<groupId>be.fedict.eid-trust-service</groupId>
			<artifactId>eid-trust-service-xkms2-ws-impl</artifactId>
			<version>${project.version}</version>
